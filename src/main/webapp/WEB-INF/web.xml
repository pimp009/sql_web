<?xml version="1.0" encoding="windows-1251"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>MyFirstServlet</display-name>

    <servlet>
        <servlet-name>dairy</servlet-name>
        <servlet-class>ru.innopolis.stc9.Servelt.ServeletDairy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dairy</servlet-name>
        <url-pattern>/dairy</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>student</servlet-name>
        <servlet-class>ru.innopolis.stc9.Servelt.ServletStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>student</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>group</servlet-name>
        <servlet-class>ru.innopolis.stc9.Servelt.ServletGroup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>group</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>

</web-app>

